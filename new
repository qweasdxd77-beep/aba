local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local RunService = game:GetService("RunService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local Players = game:GetService("Players")
local GuiService = game:GetService("GuiService")
local UserInputService = game:GetService("UserInputService")
local ContextActionService = game:GetService("ContextActionService")
local VirtualUser = game:GetService("VirtualUser")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera
local Workspace = game:GetService("Workspace")

-- [[ Global Variables ]] --
getgenv().IsMain = false
getgenv().IsAlt = false
getgenv().InfDash = false 
getgenv().AutoResetAlt = false 
getgenv().ResetDelay = 1

-- [[ Smart Dash Variable ]] --
getgenv().OppositeDash = false

-- [[ Hitbox Variables ]] --
getgenv().AkainuHitbox = false
getgenv().AkainuSize = 100

-- [[ Aim Lock Variables ]] --
getgenv().AimSystemEnabled = false 
getgenv().IsLocking = false 
getgenv().LockedTarget = nil 

-- [[ Dash Settings ]] --
local DashAnimID = "rbxassetid://1357408709" 
local DashTime = 0.35 
local CurrentDashSpeed = 100 

-- [[ Aim Lock Settings ]] --
local AimPart = "HumanoidRootPart"
local AimSmooth = 0.5 

-- [[ Anti-AFK ]] --
LocalPlayer.Idled:Connect(function()
    if getgenv().IsMain or getgenv().IsAlt then
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
end)

task.spawn(function()
    while true do
        task.wait(600)
        pcall(function()
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.Jump = true
            end
        end)
    end
end)

local Window = Rayfield:CreateWindow({
    Name = "ABA Script",
    Icon = 0,
    LoadingTitle = "ABA Script",
    LoadingSubtitle = "By rfaide",
    Theme = "Default",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,
    ConfigurationSaving = { Enabled = true, FileName = "ABAFarmCombatConfig" },
